{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2018",
		"lib": [
			"es2018"
		],
		"sourceMap": false,
		"declaration": false,
		"experimentalDecorators": true,
		"noImplicitReturns": true,
		"pretty": true,
		"skipLibCheck": true,
		"typeRoots": [
			"node_modules/@types"
		]
	},
	"include": [
		"./Oracle/**/*", "*.ts"
	]
}